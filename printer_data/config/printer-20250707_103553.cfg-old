gcode:
  SET_LED LED="hotend_rgb" RED=0.1 GREEN=0.00 BLUE=0.0 WHITE=0.0 INDEX=1 SYNC=0 TRANSMIT=0
  SET_LED LED="hotend_rgb" RED=0.0 GREEN=0.0 BLUE=0.0 WHITE=1.0 INDEX=2 SYNC=0 TRANSMIT=0
  SET_LED LED="hotend_rgb" RED=0.0 GREEN=0.0 BLUE=0.0 WHITE=1.0 INDEX=3 SYNC=0 TRANSMIT=1

[gcode_macro LED_OFF]
