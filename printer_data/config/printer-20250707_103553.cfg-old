[include mainsail.cfg]                  # Include configuration from mainsail.cfg file
[include MotorSync.cfg]
[include quattrobox_mmb.cfg]

[virtual_sdcard]
path: /home/biqu/printer_data/gcodes    # Path to the virtual SD card directory
on_error_gcode: CANCEL_PRINT            # G-code to execute on error (defined in CANCEL_PRINT macro)

[gcode_arcs]
resolution: 0.1                         # Arc interpolation resolution, handles G2/G3 commands

[exclude_object]                        # Exclude object for cancellation

[force_move]
enable_force_move: true                 # Enable force move feature

[extended_template]                     # Enables extended_macro
# The path to your extended_template config file.
# If you do not plan on creating your own Python scripts, this is not required.
# See example/function_config.yaml for the configuration schema.
path: /home/biqu/printer_data/functions/config.yaml

#####################################################################
##	                 Fysetc H36 Config
#####################################################################
[mcu H36Can]
canbus_uuid: dcfc08b5ac16              # Unique identifier for the CAN bus interface

# IO.0: PA15
# IO.1: PC7
# IO.2: PA8
# IO.3/RGB0: PA10
# IO.4/RGB1: PB1
# IO.0 - IO.5 with level conversion (pull-up), it can be used as 5V/24V level input and output!

# FAN0: PA9
# FAN0_TACH: PC6 # Please make sure the feedback level does not exceed 5V!
# FAN1: PB15
# FAN1_TACH: PB14 # Please make sure the feedback level does not exceed 5V!
# FAN2: PB13
# FAN3: PA5
# FAN3_TACH: PB0 # Please make sure the feedback level does not exceed 5V!

# Heat: PA7
# Thermistor (2.2k pull up): PA6

# Driver_EN: PB7
# Driver_DIR: PB8
# Driver_STEP: PB9
# Driver_UART: PC14
# Driver_DIAG: PC15

# ADXL345
# spi_bus: spi2
# adxl345_cs_pin: PB12
# adxl345_spi_software_sclk_pin: PB10
# adxl345_spi_software_mosi_pin: PB11
# adxl345_spi_software_miso_pin: PB2

# CAN_RX: PD0
# CAN_TX: PD1

# 5V_Monitor: PA0
# 24V_MOnitor: PA4
# Thermistor next to TMC2209 (4.7k pull up) : PA3

# CAN USB switch: PA2
# logic Low = COM to A PORT  CANBUS
# Logic High = COM to B PORT  USB

